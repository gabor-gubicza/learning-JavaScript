const age = 17;
const isOldEnough = age >= 18;

if(isOldEnough) {
    console.log('Sarah can start a driving license ðŸš—')
} else {
    const yersLeft = 18 - age;
    console.log(`Sarah can't drive yet. She has to wait ${yersLeft} years`)
}

